        # Step 4: Calculate Lengths
                

    

        # Step 2: Update Counts Array
        def isTrue(self,word,chars_arr):
            c = [0]*26
            for ch in word:
                x = ord(ch)-ord('a')
                c[x]+=1
                if c[x]>chars_arr[x]:
                    return False
            return True

